{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@modules/*": ["modules/*"],
      "@components/*": ["components/*"],
      "@layout/*": ["layout/*"],
      "@actions/*": ["actions/*"],
      "@actionTypes/*": ["actionTypes/*"],
      "@reducers/*": ["./reducers/*"],
      "@sagas/*": ["./sagas/*"],
      "@static/*": ["static/*"],
      "@store/*": ["store/*"],
      "@dummy/*": ["__dummy__/*"]
    }
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    
  ]
}