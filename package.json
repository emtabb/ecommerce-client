{
  "name": "espace-ecommerce",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4000",
    "ssg": "next build && next dev",
    "build": "next build",
    "start": "next start -p 4000",
    "build:dev": "NODE_ENV=development next build",
    "build:production": "NODE_ENV=production next build",
    "start:development": "NODE_ENV=development ENV=development node server.js",
    "start:dev-win": "set NODE_ENV=development && node server.js",
    "build:dev-win": "set NODE_ENV=development && next build",
    "start:production-win": "set NODE_ENV=production && node server.js",
    "start:production": "NODE_ENV=production ENV=production node server.js",
    "start-prod": "next start -p 443",
    "lint": "next lint",
    "clean:unix": "rm -rf .next"
  },
  "dependencies": {
    "bootstrap": "^5.1.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "gray-matter": "^4.0.3",
    "http-proxy-middleware": "^2.0.1",
    "next": "^11.1.3-canary.15",
    "react": "17.0.2",
    "react-bootstrap": "^1.6.3",
    "react-dom": "17.0.2",
    "react-horizontal-scrolling-menu": "^2.2.0",
    "react-icons": "^4.3.1",
    "react-markdown": "^6.0.3",
    "react-slick": "^0.28.1",
    "remark": "^13.0.0",
    "remark-html": "^13.0.1",
    "slick-carousel": "^1.8.1",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "axios": "^0.21.4",
    "babel-plugin-styled-components": "^2.0.2",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "next-mdx-remote": "^3.0.4",
    "react-markdown": "^6.0.3",
    "remark": "^13.0.0",
    "remark-html": "^13.0.1"
  }
